# -*- coding: utf-8 -*-
# UTF-8 encoding when using korean
N1, N2 = map(int, input().split())
D = int(input())

for i in range(1,D+1):
	if i%2==1: # 홀수날 구름>바람
		if N1%2==1:
			N2+=N1//2+1
			N1=N1//2
		else:
			N2+=N1//2
			N1=N1//2
	else:
		if N2%2==1:
			N1+=N2//2+1
			N2=N2//2
		else:
			N1+=N2//2
			N2=N2//2

print(N1, N2)